

<h2>All Bands</h2>
<ul>
<%  @bands.each do |band| %>
    <li>
        <a href="<%= band_url(band) %>">
            <p><%= band.name %></p>
        </a>
    </li>

<% end %>
</ul>


<form action="<%= new_band_url %>" method="GET">
    <%= auth_token %>
    <input type="submit" value="New Band"  class="button">
</form>

